<Window x:Class="FleetManagement.UI.AddForms.AddLicenseTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FleetManagement.UI.AddForms"
        mc:Ignorable="d"
        Title="AddLicenseTypeWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Label Content="Add License Type" Grid.Row="0" FontSize="28" Margin="5"/>
        <ListBox x:Name="lstLicenseTypes" Grid.Row="1" Margin="16"/>
        <Button x:Name="btnAddLicenseType" Grid.Row="2" Margin="16" FontSize="20" Content="Add License" Padding="5" Click="btnAddFuelType_Click"/>
    </Grid>
</Window>
